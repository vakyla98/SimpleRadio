{
    "root": true,
    "env": {
        "node": true
    },
    "extends": ["plugin:vue/essential", "eslint:recommended"],
    "parserOptions": {
        "parser": "babel-eslint"
    },
    "rules": {
        "no-console": ["warn", { "allow": ["warn", "error"] }],
        "no-prototype-builtins": 0
    }
}
